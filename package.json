{
  "name": "kala-sarthi",
  "version": "0.1.1",
  "private": true,
  "overrides": {
    "@browserbasehq/stagehand": {
      "openai": "$openai"
    }
  },
  "scripts": {
    "dev": "next dev -p 9003 --turbo",
    "dev:fast": "NODE_OPTIONS='--max-old-space-size=4096' next dev -p 9003 --turbo",
    "genkit:dev": "genkit start -- tsx src/ai/dev.ts",
    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
    "ai:init": "tsx src/ai/initialize.ts",
    "db:sample": "tsx -e \"import('./src/lib/database/buyer-connect-init.ts').then(m => m.createSampleData())\"",
    "db:cleanup": "tsx -e \"import('./src/lib/database/buyer-connect-init.ts').then(m => m.cleanupSampleData())\"",
    "seed:firestore": "node scripts/run-seed.js",
    "migrate:vectors": "node scripts/migrate-to-firestore-vectors.js",
    "migrate:complete": "node scripts/complete-firestore-migration.js",
    "build": "NODE_OPTIONS='--max-old-space-size=8192' next build",
    "start": "next start -p 8080",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:performance": "jest --testPathPattern=performance --runInBand --detectOpenHandles",
    "test:performance:quick": "tsx src/__tests__/performance/run-performance-tests.ts --quick",
    "test:performance:full": "tsx src/__tests__/performance/run-performance-tests.ts",
    "test:load": "tsx src/__tests__/performance/load-test-runner.ts",
    "test:stress": "tsx src/__tests__/performance/stress-test-runner.ts",
    "clean": "rm -rf .next && rm -rf node_modules/.cache",
    "dev:optimize": "node scripts/dev-optimize.js && npm run dev:fast",
    "update:products-status": "node scripts/update-products-status.js",
    "update:products-status:ts": "tsx scripts/update-products-status.ts"
  },
  "dependencies": {
